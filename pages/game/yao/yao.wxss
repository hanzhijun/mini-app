page {
  background: #fff!important;
}
.yao {
  display: none;
  width: 750rpx;
  overflow: hidden;
}

.yao.this-show {
  display: block;
}

.goods-detail {
  overflow: hidden;
  background-color: #fff;
  padding: 20rpx 0;
  border-bottom: 6rpx solid #f5f5f5;
}

.goods-detail .goods-img {
  width: 190rpx;
  height: 190rpx;
  float: left;
  margin-left: 15rpx;
}

.goods-detail .goods-img image {
  width: 190rpx;
  height: 190rpx;
}

.goods-detail .goods-main {
  width: 520rpx;
  height: 150rpx;
  float: left;
  margin-left: 20rpx;
}

.goods-detail .goods-main .goods-name {
  line-height: 40rpx;
  height: 80rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  text-overflow: ellipsis;
}

.game-ts {
  overflow: hidden;
  padding: 20rpx 35rpx;
}

.game-ts .game-text {
  padding: 10rpx;
  text-align: center;
}

.goods-price {
  padding-top: 10rpx;
}

.goods-price .info {
  line-height: 35rpx;
}

.game-bar {
  width: 750rpx;
  height: 550rpx;
  text-align: center;
  position: relative;
}

.game-bar .yao-out {
  width: 550rpx;
  height: 550rpx;
  background: url(https://img.duishangbao.cn/upload/game/guan_11.jpg?v=1016) no-repeat 0 0 / 550rpx 550rpx;
  position: absolute;
  left: 100rpx;
}

.game-bar .yao-out .yao-gai {
  width: 550rpx;
  height: 550rpx;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 3;
}

.game-bar .yao-out.gai-dou {
    animation: mydou 0.02s infinite;
    animation-direction:alternate;
}
@keyframes mydou {
    0%   {left: 90rpx;}
    100% {left: 120rpx;}
}

.game-bar .yao-out .yao-gai.gai-move {
    animation: mymove 2s linear;
    -webkit-animation-iteration-count:1;
    -webkit-animation-fill-mode:forwards;
}
@keyframes mymove {
    0%   {left: 0;top:0}
    60%  {left: 0;top:0}
    100% {left: 1500rpx;top:-200rpx}
}

.game-bar .yao-out text {
  display: inline-block;
  width: 123rpx;
  height: 123rpx;
  position: absolute;
  top: 248rpx;
  left: 135rpx;
}

.game-bar .yao-out text.shai-right {
  left: 302rpx;
}

.game-bar .yao-out text.shai-1 {
  background: url(https://img.duishangbao.cn/upload/game/shai_1.jpg) no-repeat 0 0 / 123rpx 123rpx;
}
.game-bar .yao-out text.shai-2 {
  background: url(https://img.duishangbao.cn/upload/game/shai_2.jpg) no-repeat 0 0 / 123rpx 123rpx;
}
.game-bar .yao-out text.shai-3 {
  background: url(https://img.duishangbao.cn/upload/game/shai_3.jpg) no-repeat 0 0 / 123rpx 123rpx;
}
.game-bar .yao-out text.shai-4 {
  background: url(https://img.duishangbao.cn/upload/game/shai_4.jpg) no-repeat 0 0 / 123rpx 123rpx;
}
.game-bar .yao-out text.shai-5 {
  background: url(https://img.duishangbao.cn/upload/game/shai_5.jpg) no-repeat 0 0 / 123rpx 123rpx;
}
.game-bar .yao-out text.shai-6 {
  background: url(https://img.duishangbao.cn/upload/game/shai_6.jpg) no-repeat 0 0 / 123rpx 123rpx;
}

.game-time {
  text-align: center;
  padding: 20rpx 0 30rpx;
}

.game-btn {
  padding-top: 30rpx;
}

.game-btn button {
  width: 90vw;
  height: 10vw;
  line-height: 10vw;
  text-align: center;
  margin: 25rpx auto;
}

.game-btn .game-state {
  display: block;
