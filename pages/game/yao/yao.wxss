  margin: 15rpx auto;
  overflow: hidden;
}

.game-btn .btn-info.my-pk {
  width: 234rpx;
  height: 81rpx;
  line-height: 300rpx;
  background: url(https://img.duishangbao.cn/upload/game/btn_mypk.jpg?v=1025) no-repeat 0 0 / 234rpx 81rpx;
  margin: 15rpx auto;
  overflow: hidden;
}

.game-btn .btn-yaoyiyao {
  width: 234rpx;
  height: 81rpx;
  line-height: 300rpx;
  background: url(https://img.duishangbao.cn/upload/game/btn_yaoyiyao.jpg) no-repeat 0 0 / 234rpx 81rpx;
  margin: 15rpx auto;
  overflow: hidden;
}

.game-btn .btn-yaoyiyao2 {
  width: 234rpx;
  height: 81rpx;
  line-height: 300rpx;
  background: url(https://img.duishangbao.cn/upload/game/btn_yaoyiyao2.jpg) no-repeat 0 0 / 234rpx 81rpx;
  margin: 15rpx auto;
  overflow: hidden;
}

.game-btn .btn-yaoqing {
  width: 305rpx;
  height: 81rpx;
  background: url(https://img.duishangbao.cn/upload/game/btn_yaoqing.jpg) no-repeat 0 0 / 305rpx 81rpx;
