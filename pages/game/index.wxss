}

.pop {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.8);
  z-index: 99999;
  width: 100vw;
  height: 100vh;
}

.pop-main {
  width: 570rpx;
  position: absolute;
  top: calc(50vh - 280rpx);
  left: 90rpx;
  background: white;
  border-radius: 15rpx;
  text-align: center;
  overflow: hidden;
  padding-bottom: 60rpx;
}

.close {
  color: #444;
  font-size: 30px;
  width: 30px;
  height: 30px;
  line-height: 30px;
  text-align: center;
  position: absolute;
  right: 2rpx;
  top: 2rpx;
  z-index: 3;
}

.colse:active {
  opacity: .8
}

.pop-title {
