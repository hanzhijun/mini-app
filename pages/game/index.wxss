}

.good_title {
  /* width:calc((100vw - 45rpx) / 2); */
  display: block;
  color: #fff;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  padding: 5rpx 0 0 5rpx;
  box-sizing: border-box;
  text-align: left;
  min-height: 80rpx;
}

.good_price {
  padding-left: 10rpx;
