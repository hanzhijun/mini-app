  margin-top: 43rpx;
}

.tc-base .tc-04 text:last-child {
  font-size: 28rpx;
  color: #666;
}

.butBtn {
  position: absolute;
  bottom:0;
  height: 120rpx;
  width: 599rpx;
  display: flex;
  justify-content: space-around;
  text-align: center;
  border-top: 1px solid #ccc;
  margin-top: 36rpx;
  overflow: hidden;
}

.butBtn button {
  padding: 0 !important;
  margin: 0 !important;
  background: #fff;
  width: 50%;
  line-height: 120rpx;
}

.butBtn button:first-child {
  border-right: 1px solid #ccc;
  color: #666;
  font-size: 36rpx;
  border-radius: 0 0 0 15rpx;
}

.butBtn button:last-child {
  color: #e62343;
  font-size: 36rpx;
  border-radius: 0 0 15rpx  0;
}

button::after {
  border: none;
}

.shareBtn{
  padding: 0 !important;
  width: 455rpx;
  height: 75rpx;
  background: #e62343;
  color: #fff;
  line-height: 75rpx;
  font-size: 34rpx;
  margin-top: 30rpx;
}

.tc-base .tc-03 .isee:active, .return:active, .btnClass:active,
.btnClass2:active, .rule:active, .tc-base .tc-02 button:active,
.tc-base .tc-01 button:active {
  opacity: 0.8;
}

.ruleContent {
  position: absolute;
  width: 670rpx;
  height: calc(100vh - 80rpx);
  line-height: 40rpx;
  top: 20rpx;
  left: 20rpx;
  background: rgba(255, 255, 255, 0.9);
  color: #333;
  z-index: 2;
  font-size: 24rpx;
  overflow: scroll;
  padding: 20rpx;
  border-radius: 8px;
  overflow-y: auto;
}

.dividerLine {
  width: 85vw;
  border-top: 1px solid #e5e5e5;
  margin: 25rpx auto;
}

.popTitle{
  color:#e62343;
  font-size: 40rpx;
  margin-top: 45rpx;
}

.getGoldNum{
  color: #666666;
  font-size: 32rpx;
  margin-top: 20rpx;
}

.mar{
  margin-top: 50rpx;
}

