/** loading toast **/
.loading-bar {
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 101;
}

.loading-bar .gray-back {
  width: 100%;
  height: 100%;
  background-color: #fff;
  opacity: 0;
  position: absolute;
  left: 0;
  top: 0;
}

.loading-bar .black-back {
  width: 120px;
  height: 120px;
  background: rgba(0, 0, 0, 0.5);
  position: absolute;
  left: calc(50% - 60px);
  top: calc(50% - 60px);
  border-radius: 5px;
}

.loading-bar .black-back .img {
  text-align: center;
  padding: 30px 0 20px;
}

.loading-bar .black-back .img image {
  width: 30px;
  height: 30px;
}

.loading-bar .black-back .txt {
  text-align: center;
  font-size: 12px;
  color: #f0f0f0;
}

.show-toast {
  width: 100%;
  height: 20px;
  text-align: center;
  position: fixed;
  left: 0;
  bottom: 180px;
  z-index: 10011;
  overflow: visible;
}

.show-toast text {
  display: inline-block;
  padding: 9px 15px;
  background-color: #333;
  color: #fff;
  font-size: 14px;
  margin: 0 auto;
  border-radius: 5px;
  max-width: 60vw;
}